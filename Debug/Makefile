CC=g++
FLAGS=-std=c++17 -Wall -g

all:
	@echo "There is no \"make all\" for this project."
	@echo "You can use \"make help\" to see the list of commands available\n\n"

help:
	@echo "\nHere is a list of available makes"
	@echo "================================="
	@echo "0. Plan - see what the plan is for today"
	@echo "1. Arrays - builds Arrays/ directory"
	@echo "2. Conditions - builds Conditions/ directory"
	@echo "3. Classes - builds Classes/ directory"
	@echo "4. Functions - builds Functions/ directory"
	@echo "5. Challenges - builds Challenges/ directory"	

